<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="using:LaunchpadSamplingApp.ViewModels"
             xmlns:views="using:LaunchpadSamplingApp.Views"
             x:Class="LaunchpadSamplingApp.App"
             RequestedThemeVariant="Dark">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

  <Application.Styles>
    <FluentTheme/>
    
    <StyleInclude Source="/Styles/NovationLogo.axaml"/>
  </Application.Styles>

  <Application.Resources>
    <ResourceDictionary>
      
      <ResourceDictionary.MergedDictionaries>
        <ResourceInclude Source="/Styles/DarkTheme.axaml"/>
        
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
</Application>
