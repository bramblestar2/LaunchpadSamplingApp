<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Add Resources Here -->

  <!--Base Colors-->
  <SolidColorBrush x:Key="primaryBackgroundColor" Color="#606060"/>
  <SolidColorBrush x:Key="secondaryBackgroundColor" Color="#6A6A6A"/>


  <!--Launchpad Colors-->
  <Brush x:Key="LaunchpadButtonIdle">#505050</Brush>
  <Brush x:Key="LaunchpadButtonHover">#404040</Brush>
  <Brush x:Key="LaunchpadButtonPress">#303030</Brush>
  
  <SolidColorBrush x:Key="LaunchpadPrimaryBackground" Color="#555555"/>
  <SolidColorBrush x:Key="LaunchpadSecondaryBackground" Color="#454545"/>
  
</ResourceDictionary>
